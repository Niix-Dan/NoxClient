{
  "schemaVersion": 1,
  "name": "Nox Client",
  "id": "nox-client",
  "version": "1.1.0",
  "description": "Based utility mod.",
  "authors": [
    "Niix"
  ],
  "contact": {
    "homepage": "/",
    "discord": "/"
  },
  "license": "GPL-3.0",
  "icon": "assets/nox-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.noxclient.NoxClient"
    ]
  },
  "mixins": [
    "nox-client.mixins.json",
    "nox-client-baritone.mixins.json",
    "nox-client-indigo.mixins.json",
    "nox-client-sodium.mixins.json",
    "nox-client-canvas.mixins.json"
